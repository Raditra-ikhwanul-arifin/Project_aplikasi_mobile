<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="290.62dp"
        android:layout_height="90.34dp"
        android:fontFamily="@font/ribeyeregular"
        android:text="Set Schedule"
        android:textColor="#000000"
        android:textSize="36dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="265dp" />

    <ImageView
        android:layout_width="208dp"
        android:layout_height="165dp"
        android:src="@drawable/logo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="110dp" />

    <EditText
        android:id="@+id/nameInput"
        style="@style/ModernLabel"
        android:layout_width="329dp"
        android:layout_height="58dp"
        android:background="@drawable/rounded_edit_text_background"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="425dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/interbold"
        android:text="Masukkan Nama"
        android:textColor="#000000"
        android:textSize="19dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="385dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="205.45dp"
        android:layout_height="78.01dp"
        android:backgroundTint="#555151"
        android:text="Masuk"
        android:textColor="#FFFFFF"
        android:textSize="24dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="560dp" />


</androidx.constraintlayout.widget.ConstraintLayout>